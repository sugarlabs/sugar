#: ../data/org.sugarlabs.gschema.xml.h:80 ../data/sugar.schemas.in.h:52
msgid "GSM network password configuration (DEPRECATED/UNUSED)"
msgstr "Cano odoco i mung me network pa GSM (KITIYO KWEDE KU/PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:81 ../data/sugar.schemas.in.h:53
msgid "GSM network number (DEPRECATED/UNUSED)"
msgstr "Namba me network pa GSM (KITIYO KWEDE KU/PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:82 ../data/sugar.schemas.in.h:54
msgid "GSM network telephone number configuration (DEPRECATED/UNUSED)"
msgstr ""
"Cano odoco namba me cim pa network pa GSM (KITIYO KWEDE KU/PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:83 ../data/sugar.schemas.in.h:55
msgid "GSM network APN (DEPRECATED/UNUSED)"
msgstr "GSM network APN (KITIYO KWEDE KU/PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:84 ../data/sugar.schemas.in.h:56
msgid "GSM network access point name configuration (DEPRECATED/UNUSED)"
msgstr ""
"Cano odoco ka nongo network pa GSM kakare (KITIYO KWEDE KU/PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:85 ../data/sugar.schemas.in.h:57
msgid "GSM network PIN (DEPRECATED/UNUSED)"
msgstr "PIN me network pa GSM (KITIYO KWEDE KU/PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:86 ../data/sugar.schemas.in.h:58
msgid ""
"GSM network personal identification number configuration (DEPRECATED/UNUSED)"
msgstr ""
"Cano odoco namba me tikitiki ma kamegi pi network pa GSM ( KITIYO KWEDE KU/"
"PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:87 ../data/sugar.schemas.in.h:59
msgid "GSM network PUK (DEPRECATED/UNUSED)"
msgstr "PUK me network pa GSM (KITIYO KWEDE KU/PE KITIYO KWEDE)"

#: ../data/org.sugarlabs.gschema.xml.h:88 ../data/sugar.schemas.in.h:60
msgid "GSM network personal unlock key configuration (DEPRECATED/UNUSED)"
msgstr ""
"Cano odoco lagony me yabo network mamegi pa GSM (KITIYO KWEDE KU/PE KITIYO "
"KWEDE)"

