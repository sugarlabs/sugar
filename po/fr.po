#, python-format
msgid "fill:     color=%s hue=%s"
msgstr "remplissage:     couleur=%s teinte=%s"

#: ../extensions/cpsection/aboutme/model.py:90
#, python-format
msgid "fill:     %s"
msgstr "remplissage:     %s"

#: ../extensions/cpsection/aboutme/model.py:102
msgid "Error in specified color modifiers."
msgstr "Erreur dans les modificateurs de couleur spécifiés."

#: ../extensions/cpsection/aboutme/model.py:105
msgid "Error in specified colors."
msgstr "Erreur dans les couleurs spécifiées."

#: ../extensions/cpsection/aboutme/view.py:243
msgid "Click to change your color:"
msgstr "Cliquer pour changer de couleur :"

#: ../extensions/cpsection/aboutme/view.py:294
#: ../src/jarabe/intro/window.py:203
msgid "Select gender:"
msgstr "Sélectionner un genre:"

#: ../extensions/cpsection/aboutcomputer/__init__.py:21
msgid "About my Computer"
msgstr "À propos de mon ordinateur"

#: ../extensions/cpsection/aboutcomputer/model.py:37
msgid "Not available"
msgstr "Non disponible"

#: ../extensions/cpsection/aboutcomputer/view.py:76
msgid "Identity"
msgstr "Identité"

#: ../extensions/cpsection/aboutcomputer/view.py:87
msgid "Model:"
msgstr "Modèle:"

